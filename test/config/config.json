{
  "plugins": {
    "sample": {
      "module": "path:plugin/sample"
    },
    "preDriver": {
      "module": "path:plugin/sample",
      "priority": 99,
      "arguments": ["preDriver"]
    },
    "postDriver": {
      "module": "path:plugin/sample",
      "priority": 101,
      "arguments": ["postDriver"]
    }
  },
  "driver": {
    "browser": "phantomjs",
    "local": true,
    "jar": "/usr/local/bin/selenium-server-standalone.jar"
  },
  "data": {
    "baseUrl": "https://www.google.com",
    "passThroughFromJson": true,
    "baseDirectory": "path:."
  }
}